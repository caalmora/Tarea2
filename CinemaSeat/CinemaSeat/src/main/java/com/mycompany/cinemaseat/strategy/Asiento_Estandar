package com.mycompany.cinemaseat.strategy;

public class Asiento_Estandar implements TipoAsiento {
    private double precio;

    public Asiento_Estandar() {
        this.precio = 10.0; 
    }

    @Override
    public double calcularPrecio() {
        return precio; 
    }

    @Override
    public void setPrecio(double precio) {
        this.precio = precio;
    }
}
